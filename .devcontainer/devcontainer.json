{
  "name": "CUDD",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "ms-vscode.cpptools",
        "ms-vscode.cmake-tools"
      ]
    },
    "c_cpp_properties": {
      "configurations": [
        {
          "name": "Linux",
          "compileCommands": [
            "${workspaceFolder}/compile_commands.json"
          ]
        }
      ],
      "version": 4
    }
  }
}